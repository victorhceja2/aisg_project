# .env - Configuración de entorno para FastAPI
APP_ENV=development
APP_NAME=AISG Backend
APP_VERSION=1.0.0

# Base de datos (ajusta los valores reales de tu SQL Server)
DB_SERVER=192.168.1.100  # IP de tu servidor SQL Server
DB_NAME=aisg_db
DB_USER=tu_usuario
DB_PASSWORD=tu_contraseña
DB_DRIVER=ODBC Driver 17 for SQL Server

# Puerto (opcional si quieres que FastAPI escuche otro puerto dentro del contenedor)
PORT=8000